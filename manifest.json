{
  "name": "Translator",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["webNavigation", "storage", "contextMenus", "activeTab"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["build/distributions/locales-ext.js"],
    "css": ["./index.css"]
  }],
  "web_accessible_resources": ["build/distributions/locales-ext.js"],
  "manifest_version": 2
}
